{
  "version": 2,
  "builds": [
    {
      "src": "Desktop/2025 Portfolio Project/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/Desktop/2025 Portfolio Project/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/",
      "dest": "/Desktop/2025 Portfolio Project/index.html"
    }
  ]
}
